public class ClassWithProperty {
    protected int intProp;

    private String privateStringProp;

    private String stringProp;

    /**
     * Get the intProp property.
     * @return intProp.
     */
    protected int getIntProp() {
        return this.intProp
    }

    /**
     * Get the privateStringProp property.
     * @return privateStringProp.
     */
    private String getPrivateStringProp() {
        return this.privateStringProp
    }

    /**
     * Get the stringProp property.
     * @return stringProp.
     */
    public String getStringProp() {
        return this.stringProp
    }

    /**
     * Set intProp property.
     * @param value Property value.
     */
    protected void setIntProp(int value) {
        this.intProp = value;
    }

    /**
     * Set privateStringProp property.
     * @param value Property value.
     */
    private void setPrivateStringProp(String value) {
        this.privateStringProp = value;
    }

    /**
     * Set stringProp property.
     * @param value Property value.
     */
    public void setStringProp(String value) {
        this.stringProp = value;
    }
}
